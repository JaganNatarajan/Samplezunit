 "zopeneditor.userbuild.workspaceSettings": {
    "dbb": {
        "command": "$DBB_HOME/bin/groovyz",
        "buildScriptPath": "/u/<userID>/dbb-zappbuild/build.groovy",
        "buildScriptArgs": [
            "--userBuild",
            "--workspace ${zopeneditor.userbuild.userSettings.dbbWorkspace}",
            "--application cics-genapp",
            "--hlq ${zopeneditor.userbuild.userSettings.dbbHlq}",
            "--outDir ${zopeneditor.userbuild.userSettings.dbbWorkspace}/logs"
        ],
        "additionalDependencies": [
            "application-conf"
        ]
    }
},
    "zopeneditor.userbuild.userSettings": {
        "dbbWorkspace": "/u/<userID>/USERBUILD",
        "dbbHlq": "<userID>",
        "dbbLogDir": "/u/<userID>/USERBUILD/logs",
        "localWorkspacePath": "/projects/cics-genapp"
    }
}
